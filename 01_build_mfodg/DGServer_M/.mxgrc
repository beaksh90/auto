# JAVA home directory
JAVA_HOME=/home/

# DataGather_Master home directory
MXG_HOME=/home/maxgauge/
DG_M_HOME=/home/maxgauge/DGServer_M
DG_M_NAME=DG_M

# Add DataGather bin to $PATH
PATH=${JAVA_HOME}/bin:${MXG_HOME}/bin:${DG_M_HOME}/bin:$PATH

# ksh : Export Variables

export DG_M_HOME DG_M_NAME MXG_HOME PATH

alias bin="cd $DG_M_HOME/bin"
alias conf="cd $DG_M_HOME/conf"
alias log="cd $DG_M_HOME/log"
