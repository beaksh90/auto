# DataGather_Slave1 home directory
JAVA_HOME=/home/

# /home/admin/MaxGauge5.3.2_160905.01.package/DGServer_S1
MXG_HOME=/home/
DG_NAME=DG_S1

# Heap Memory ( unit : MB )
DG_XMS=3072
DG_XMX=3072

#OS_TYPE ( aix | hpia | hppa | linux32 | linux64 | sun32 | sun64 )
OS_TYPE=

# Add DataGather bin to $PATH
PATH=${JAVA_HOME}/bin:${MXG_HOME}/bin:$PATH

# ksh : Export Variables

export DG_NAME DG_XMS DG_XMX MXG_HOME OS_TYPE PATH

alias bin="cd $MXG_HOME/bin"
alias conf="cd $MXG_HOME/conf"
alias log="cd $MXG_HOME/log"
